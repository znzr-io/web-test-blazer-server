@namespace web_test_blazer_server.Shared.Airtable


<div class="row align-items-start">
    <div class="btn-group" role="group" aria-label="row controls">
        <div class="col-1">
            <div class="row align-items-start">
                <div class="btn-group" role="group" aria-label="row bookmark">
                    <A_RowBookmark></A_RowBookmark>
                </div>
            </div>
        </div>

        <div class="col-8">
            <div class="row align-items-start">
                    <P_Row IdMenuItem="@IdMenuItem" IdParentMenu="@IdParentMenu"></P_Row>
            </div>
        </div>

        <div class="col-2">
            <div class="row align-items-start">
                <div class="btn-group" role="group" aria-label="row delete & duplicate">
                    <A_RowDelete></A_RowDelete>
                    <A_RowDuplicate></A_RowDuplicate>
                </div>
            </div>
        </div>
    </div>
</div>






