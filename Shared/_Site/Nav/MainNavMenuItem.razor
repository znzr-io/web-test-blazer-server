@using web_test_blazer_server.Data.Nav
@using web_test_blazer_server.Design;
@namespace web_test_blazer_server.Shared._Site
@inject MainNavMenuService MainNavMenuService



@if(Menu != null)
    @foreach(var nav in Menu)
    {
        <div class="nav-item px-3">
            <NavLink class="nav-link" href="@nav.url" Match="NavLinkMatch.All">
                <span class="oi @web_test_blazer_server.Design.Icon.Convert(nav.icon.ToString())" aria-hidden="true"></span> @nav.buttonText
            </NavLink>
        </div>
    }